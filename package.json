{
  "name": "log-viewer",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "tail": "tail -n 1000 -f log.log | node --inspect=0 --enable-source-maps build/server.js",
    "start": "concurrently 'npm:tsc-watch' 'npm:public:dev'",
    "tsc-watch": "tsc-watch --outDir build --noClear --onSuccess 'npm run tail'",
    "public:dev": "cd svelte && npm run dev",
    "echo": "echo \"{ '(date -Ins)' timestamp: '2021-02-13T17:48:32.841Z',  level: 'INFO',  message: 'Finished job execution5'}\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@most/adapter": "^1.0.0",
    "@most/core": "^1.6.1",
    "@most/dom-event": "^2.2.0",
    "@most/scheduler": "^1.3.0",
    "@most/types": "^1.1.0",
    "express": "^4.17.1",
    "fp-ts": "^2.9.5",
    "json5": "^2.2.0",
    "memoize-one": "^5.1.1",
    "ramda": "^0.27.1",
    "ws": "^7.4.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.27",
    "@types/ramda": "^0.27.38",
    "@types/ws": "^7.4.0",
    "parcel-bundler": "^1.12.4",
    "ts-node-dev": "^1.1.1",
    "tsc-watch": "^4.2.9",
    "typescript": "^4.1.5"
  }
}
